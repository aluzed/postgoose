{
  "name": "postgoose",
  "version": "0.0.1",
  "description": "Postgresql handler like mongoose",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/_mocha ./tests/index.js --exit",
    "dev:stats": "find . -name '*.js' -not -path './node_modules/*' -not -path './public/*' -not -path './docs/*' | xargs wc -l",
    "doc": "rm -rf ./docs && jsdoc -u ./documentation/tutorials -R ./documentation/documentation.md -c ./jsdoc.json -d docs .",
    "doc-server": "http-server ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aluzed/postgoose.git"
  },
  "keywords": [
    "postgresql",
    "mongoose"
  ],
  "author": "Alexandre PENOMBRE <aluzed_AT_gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aluzed/postgoose/issues"
  },
  "homepage": "https://github.com/aluzed/postgoose#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "pg": "^7.4.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.5",
    "marked": "^0.3.19",
    "mocha": "^5.0.4",
    "tui-jsdoc-template": "^1.2.2"
  }
}
